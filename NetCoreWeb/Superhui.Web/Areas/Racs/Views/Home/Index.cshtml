@model Newtonsoft.Json.Linq.JArray
<h2>Android packages download</h2>
<table class="table table-bordered table-striped">
    <tr>
        <th>名称</th>
        <th>大小</th>
        <th>修改时间</th>
        <th>操作</th>
    </tr>
    @foreach (var f in Model) {
        <tr>
            <td>@f["name"]</td>
            <td>@string.Format("{0}", Math.Round(double.Parse((string)f["info"]["size"]) / 1024 / 1024, 2))</td>
            <td>@f["info"]["lastModified"]</td>
            <th><a href=@string.Format("/api/file/download{0}", f["path"])>download</a></th>
        </tr>
        @*<tr>
            <td>uat</td>
            <th><a href="/api/download/packages/app-release-uat.apk">download</a></th>
        </tr>
        <tr>
            <td>product</td>
            <th><a href="/api/download/packages/app-release-product.apk">download</a></th>
        </tr>*@
    }


</table>